{
  "manifest_version": 3,
  "name": "Thingiverse Auto Fill",
  "version": "1.0",
  "description": "Preenche automaticamente os campos da p√°gina 'Post a Make' no Thingiverse.",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://www.thingiverse.com/*/make"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "128": "icons/icon128.png"
  }
}
